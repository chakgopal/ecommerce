<p id="notice"><%= notice %></p>
 


<h1>Quote Items</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Quantity</th>
       <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quote_item.each do |quote_item| %>
		<tr>
			<td><%= quote_item.name %></td>
			<td><%= quote_item.description %></td>
			<td><%= quote_item.quantity %></td>
			<td><%= quote_item.price %></td>
		   
		 </tr>
    <% end %>
    <%= link_to "Admin Login", new_admin_session_path %>
  </tbody>
</table>


